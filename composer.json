{
    "name": "sylius/core",
    "type": "library",
    "description": "Sylius e-commerce core. It integrates all components.",
    "keywords": ["shop", "ecommerce", "core", "store", "sylius"],
    "homepage": "http://sylius.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Paweł Jędrzejewski",
            "homepage": "http://pjedrzejewski.com"
        },
        {
            "name": "Sylius project",
            "homepage": "http://sylius.org"
        },
        {
            "name": "Community contributions",
            "homepage": "http://github.com/Sylius/Sylius/contributors"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",

        "knplabs/gaufrette": "0.2.*@dev",
        "sylius/addressing": "dev-symfony_3_compatibility",
        "sylius/archetype": "dev-symfony_3_compatibility",
        "sylius/attribute": "dev-symfony_3_compatibility",
        "sylius/cart": "dev-symfony_3_compatibility",
        "sylius/channel": "dev-symfony_3_compatibility",
        "sylius/contact": "dev-symfony_3_compatibility",
        "sylius/currency": "dev-symfony_3_compatibility",
        "sylius/inventory": "dev-symfony_3_compatibility",
        "sylius/locale": "dev-symfony_3_compatibility",
        "sylius/order": "dev-symfony_3_compatibility",
        "sylius/originator": "dev-symfony_3_compatibility",
        "sylius/payment": "dev-symfony_3_compatibility",
        "sylius/pricing": "dev-symfony_3_compatibility",
        "sylius/product": "dev-symfony_3_compatibility",
        "sylius/promotion": "dev-symfony_3_compatibility",
        "sylius/rbac": "dev-symfony_3_compatibility",
        "sylius/registry": "dev-symfony_3_compatibility",
        "sylius/resource": "dev-symfony_3_compatibility",
        "sylius/sequence": "dev-symfony_3_compatibility",
        "sylius/shipping": "dev-symfony_3_compatibility",
        "sylius/storage": "dev-symfony_3_compatibility",
        "sylius/taxation": "dev-symfony_3_compatibility",
        "sylius/taxonomy": "dev-symfony_3_compatibility",
        "sylius/user": "dev-symfony_3_compatibility",
        "sylius/variation": "dev-symfony_3_compatibility",
        "sylius/association": "dev-symfony_3_compatibility",
        "sylius/review": "dev-symfony_3_compatibility",
        "sylius/metadata": "dev-symfony_3_compatibility",
        "webmozart/assert": "^1.0",
        "zendframework/zend-stdlib": "^2.0|^3.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "symfony/property-access": "^3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Sylius\\Component\\Core\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Sylius\\Component\\Core\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.19-dev"
        }
    }
}
